<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <!-- dikimart.kelompokbarang action window -->
    <record id="barang_action" model="ir.actions.act_window">
        <field name="name">Barang</field>
        <field name="type">ir.actions.act_window</field>
        <field name="res_model">barang.barang</field>
        <field name="view_mode">tree,form</field>
    </record>

    <menuitem
        id="barang_menu_act"
        name="Barang"
        parent="barang_menu_categ"
        action="barang_action"
        sequence="10"/>

</odoo>