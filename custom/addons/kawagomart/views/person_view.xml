<?xml version='1.0' encoding='utf-8'?>
<odoo>
  <!-- kawagomart.person tree view -->
  <record id="kawagomart_person_view_tree" model="ir.ui.view">
    <field name="name">kawagomart.person.view.tree</field>
    <field name="model">kawagomart.person</field>
    <field name="arch" type="xml">
      <tree>
        <!-- Add your fields here -->
        <field name="name"/>
        <field name="alamat" />
        <field name="tgl_lahir" />
      </tree>
    </field>
  </record>
  <!-- kawagomart.person form view -->
  <record id="kawagomart_person_view_form" model="ir.ui.view">
    <field name="name">kawagomart.person.view.form</field>
    <field name="model">kawagomart.person</field>
    <field name="arch" type="xml">
      <form string="Nama">
        <sheet>
          <group name="nama" string="Nama">
            <field name="name" />
          </group>
          <group name="data" string="Data">
            <field name="alamat" />
            <field name="tgl_lahir" />
          </group>
        </sheet>
      </form>
    </field>
  </record>
</odoo>